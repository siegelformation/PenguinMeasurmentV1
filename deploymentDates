--deployment_dates view
CREATE OR REPLACE VIEW measurement_v1.deployment_dates AS 
SELECT
  'D1' as deployment_id
, 'D1v1-D1-C' as deployment_id_detail
, 'D1v1' as deployment_id_version
, 'cross-sell' as game_type
, CAST('2020-09-21' AS date) as segment_date
, CAST('2020-09-24' AS date) as combined_offer_date
, CAST('2020-09-21' AS date) as customer_profile_date
--UNION ALL 
