--, 'cross-sell-D2-A' as deployment_id_detail
--, 'D1-A' as deployment_id_version
--, 'cross_sell' as game_type
--, CAST('2020-08-30' AS date) as segment_date
--, CAST('2020-08-24' AS date) as combined_offer_date
--, CAST('2020-08-24' AS date) as customer_profile_date
